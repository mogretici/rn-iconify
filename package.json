{
  "name": "rn-iconify",
  "version": "2.0.1",
  "description": "268,000+ Iconify icons for React Native with native MMKV caching and full TypeScript autocomplete",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./lib/typescript/index.d.ts",
      "import": "./lib/module/index.js",
      "require": "./lib/commonjs/index.js"
    },
    "./babel": {
      "types": "./lib/typescript/babel/index.d.ts",
      "import": "./lib/module/babel/index.js",
      "require": "./lib/commonjs/babel/index.js"
    },
    "./navigation": {
      "types": "./lib/typescript/navigation/index.d.ts",
      "import": "./lib/module/navigation/index.js",
      "require": "./lib/commonjs/navigation/index.js"
    },
    "./animated": {
      "types": "./lib/typescript/animated/index.d.ts",
      "import": "./lib/module/animated/index.js",
      "require": "./lib/commonjs/animated/index.js"
    }
  },
  "bin": {
    "rn-iconify": "./lib/commonjs/cli/index.js"
  },
  "files": [
    "src",
    "lib",
    "bin",
    "babel.js",
    "README.md",
    "assets",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "build": "bob build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "del-cli lib",
    "prepare": "husky",
    "prepublishOnly": "bob build",
    "generate-components": "tsx scripts/generate-components.ts",
    "release": "npm run build && npm publish"
  },
  "keywords": [
    "react-native",
    "react-native-icons",
    "icons",
    "iconify",
    "svg",
    "svg-icons",
    "icon-library",
    "icon-component",
    "typescript",
    "mmkv",
    "cache",
    "material-design",
    "heroicons",
    "lucide",
    "phosphor",
    "tabler",
    "fontawesome",
    "fluent-icons",
    "feather",
    "vector-icons"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mogretici/rn-iconify.git"
  },
  "author": "mogretici",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mogretici/rn-iconify/issues"
  },
  "homepage": "https://rn-iconify.vercel.app",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.60.0",
    "react-native-mmkv": ">=3.0.0",
    "react-native-svg": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-mmkv": {
      "optional": false
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@react-native/babel-preset": "^0.82.1",
    "@react-native/eslint-config": "^0.74.0",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.2.0",
    "react": "^18.2.0",
    "react-native": "^0.74.0",
    "react-native-builder-bob": "^0.23.0",
    "react-native-mmkv": "^3.0.0",
    "react-native-svg": "^15.0.0",
    "react-test-renderer": "^18.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.4.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "example/"
  ]
}
